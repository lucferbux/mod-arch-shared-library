{
  "name": "kubeflow-ui-monorepo",
  "version": "0.1.8",
  "description": "Monorepo containing mod-arch-shared and kubeflow-ui-essentials packages",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "test": "npm run test --workspaces",
    "lint": "npm run test:lint --workspaces",
    "build:mod-arch": "npm run build --workspace=packages/mod-arch-shared",
    "build:kubeflow": "npm run build --workspace=packages/kubeflow-ui-essentials",
    "test:mod-arch": "npm run test --workspace=packages/mod-arch-shared",
    "test:kubeflow": "npm run test --workspace=packages/kubeflow-ui-essentials",
    "clean": "rm -rf packages/*/dist packages/*/node_modules",
    "install:all": "npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/opendatahub-io/kubeflow-ui-essentials.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
